<div class="row">
    <div class="col-md-12">
        <!-- DATA TABLE -->
        <h3 class="title-5 m-b-35">List Product</h3>
        <div class="table-data__tool">
            <div class="table-data__tool-left">
                <div class="rs-select2--light rs-select2--md">
                    <select class="form-control form-control-md" name="property" tabindex="-1" aria-hidden="true" onchange="select()" id="select-category">
                        <%categorys.forEach(function(category,index){%>
                            <%if(index == 0){%> 
                                <option selected="selected" value="<%=category.SortKey%>"><%=category.SortKey%></option>
                            <%}else{%> 
                                <option value="<%=category.SortKey%>"><%=category.SortKey%></option>
                            <%}%>
                        <%})%>
                    </select>
                </div>
            </div>
            <div class="table-data__tool-right">
                <button class="au-btn au-btn-icon au-btn--green au-btn--small" onclick="getForm()">
                    <i class="zmdi zmdi-plus"></i>add item</button>
            </div>
        </div>
        <div class="table-responsive table-responsive-data2" id="admin_product">
        <%- include ListProductAdmin.ejs %>
        </div>
    </div>
</div>